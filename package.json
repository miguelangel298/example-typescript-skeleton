{
  "name": "example-typescript-skeleton",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/src/server.js",
    "start:build:staging": "npm run build && cross-env NODE_ENV=staging node dist/src/server.js",
    "start:build": "npm run build && npm run start",
    "start:dev": "nodemon dist/src/server.js",
    "build:watch": "npm run build && npm-run-all -p watch start:dev",
    "build": "./node_modules/.bin/tsc",
    "lint": "tslint -c tslint.json -p tsconfig.lint.json -t stylish --force",
    "lint:fix": "tslint -c tslint.json -p tsconfig.lint.json -t stylish --force --fix",
    "watch": "tsc -w",
    "test:unit": "jest --forceExit --verbose",
    "test": "cross-env NODE_ENV=test jest --runInBand --bail",
    "coverage": "cross-env NODE_ENV=test jest --runInBand --bail --coverage",
    "docker:debug": "docker-compose up",
    "migration:seed": "ts-node data/seeders/cli/Seed.ts",
    "migration:drop": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js schema:drop",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:rollback": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:test": "cross-env NODE_ENV=test ./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "nyc": {
    "lines": 60,
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@google-cloud/pubsub": "^1.7.3",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.2.0",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cors": "^2.8.6",
    "@types/faker": "^4.1.5",
    "@types/glob": "^7.1.1",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@singleton-core/express": "git+ssh://git@github.com:miguelangel298/singleton-core.git",
    "@types/nock": "^11.1.0",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.3",
    "@types/supertest": "^2.0.8",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.7.1",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "decode-html": "^2.0.0",
    "express": "~4.15.5",
    "faker": "^4.1.0",
    "findup-sync": "^4.0.0",
    "git": "^0.1.5",
    "glob": "latest",
    "helmet": "^3.9.0",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.33",
    "mysql2": "^3.11.0",
    "nock": "^11.3.6",
    "nyc": "^14.1.1",
    "pg": "^7.12.0",
    "puppeteer": "1.19.0",
    "requireg": "^0.2.2",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2",
    "transform-runtime": "^0.0.0",
    "ts-node": "^8.3.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/compression": "^0.0.35",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.0.39",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.49",
    "eslint-config-airbnb": "^17.1.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "nsp": "^3.1.0",
    "prettier": "^1.7.4",
    "ts-jest": "^21.1.4",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-immutable": "^4.4.0",
    "typescript": "3.0"
  }
}
